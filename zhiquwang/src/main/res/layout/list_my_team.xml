<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.icarexm.zhiquwang.custview.CircleImageView
        android:id="@+id/list_my_team_img_avatar"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginBottom="@dimen/dp_4"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_width="@dimen/dp_58"
        android:layout_height="@dimen/dp_58"
        android:src="@mipmap/ic_avatar"/>

    <TextView
        android:id="@+id/list_my_team_tv_name"
        android:layout_marginLeft="@dimen/dp_6"
        android:layout_toRightOf="@+id/list_my_team_img_avatar"
        android:layout_marginTop="@dimen/dp_29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="王女士"
        android:textColor="@color/ff4e4d4d"
        android:textSize="@dimen/sp_15"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@drawable/fff2f2f2"
        android:layout_below="@+id/list_my_team_img_avatar"/>
</RelativeLayout>
