<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/list_commission_tv_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="王二明成功注册"
        android:textColor="@color/ff4e4d4d"
        android:textSize="@dimen/sp_15"
        />
    <TextView
        android:id="@+id/list_commission_tv_time"
        android:layout_marginTop="@dimen/dp_41"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2018-04-18"
        android:textColor="@color/ffa5a5a5"
        android:textSize="@dimen/sp_12"
        />

    <View
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_below="@+id/list_commission_tv_time"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@drawable/fff2f2f2"
        />

    <TextView
        android:id="@+id/list_commission_tv_money"
        android:layout_marginTop="@dimen/dp_26"
        android:gravity="right"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_alignParentRight="true"
        android:layout_width="73dp"
        android:layout_height="22dp"
        android:text="+10.00元"
        android:textColor="#ff00b6ce"
        android:textSize="14sp"
        />
</RelativeLayout>
